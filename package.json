{
  "name": "pbs-kids-video-js-test-harness",
  "description": "PBS KIDS Video.js Test Harness",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "npm -s run postinstall && node src/server.js",
    "serve": "PORT=3000 node src/server.js",
    "watch": "npm -s run postinstall && nodemon --watch src --watch public -e js,ejs,css src/server.js",
    "lint": "eslint --ext .js ./ --fix",
    "postbuild": "chmod +x scripts/postbuild.sh && ./scripts/postbuild.sh",
    "postinstall": "scripts/postinstall.sh"
  },
  "author": "Jeff Moore",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.10",
    "eslint": "^8.57.0",
    "express": "^4.19.2",
    "md5": "^2.3.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.4",
    "request": "^2.88.2",
    "video.js": "^8.12.0",
    "videojs-contrib-dash": "^5.1.1",
    "videojs-contrib-eme": "^5.4.1"
  }
}
